<div class="paginate text-center"><ul class="pagination "><% if (paging.first) { %><li><a href="<%= paging.prelink + '?page=' + paging.first %>">&laquo;</a></li><% } %><% if (paging.previous) { %><li><a href="<%= paging.prelink + '?page=' + paging.previous %>">&lsaquo;</a></li><% } %><% for ( var i = 0; i < paging.range.length; i++ ) { %><% if(paging.range[i] == paging.current){ %><li class="active"><span><%= paging.range[i] %></span></li><% }else{ %><li><a href="<%= paging.prelink %>?page=<%= paging.range[i] %>"><%= paging.range[i] %></a></li><% } %><% } %><% if (paging.next) { %><li><a href="<%= paging.prelink + '?page=' + paging.next %>">&rsaquo;</a></li><% } %><% if (paging.last) { %><li><a href="<%= paging.prelink + '?page=' + paging.last %>">&raquo;</a></li><% } %></ul></div>